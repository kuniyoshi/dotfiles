[color]
	ui = auto
[alias]
	ap = add -p
	br = branch
	ci = commit
	co = checkout
	cop = checkout -p
	di = diff
	dicw = diff --color-words
	dicwhf = diff --color-words HEAD FETCH_HEAD
	dihf = diff HEAD FETCH_HEAD
	dino = diff --name-only
	ds = diff --staged
	g = grep
	h = help
	l = log --decorate --numstat --summary
	ll = log --decorate --numstat --summary --pretty=fuller -p
	nbr = checkout -b
	ol = log --oneline --decorate
	sh = show
	shcw = show --color-words
	st = status
	sw = checkout
	unap = reset -p
	wll = log --decorate --numstat --summary --pretty=fuller -p --word-diff
	xl = stash list
	xi = stash save
	xo = stash pop
	xd = stash drop
	xa = stash apply
	xs = stash show
	xp = stash show -p
[user]
	name = Kuniyoshi Kouji
[core]
	excludesfile = ~/.gitignore
	pager = less -R
	editor = vim
[push]
	default = matching
